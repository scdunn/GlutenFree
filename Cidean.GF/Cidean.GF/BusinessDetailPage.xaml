<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cidean.GF.BusinessDetailPage">

  <StackLayout>
    <Label Text="{Binding Business.Name}" HorizontalOptions="Center" />
    <Label Text="{Binding Business.Address}" HorizontalOptions="Center" />
    <Label Text="{Binding Business.Phone}" HorizontalOptions="Center" />
    <Label Text="{Binding Business.Url}" HorizontalOptions="Center" />

  </StackLayout>
  
 
</ContentPage>